{
  "Settings": {
    "PoolKey": "MyPoolKey",
    "PoolContractAddress": "MyPoolContractAddress",
    "FarmerKey": "MyFarmerKey",
    "DefaultPlotCommand": "/opt/chia-plotter/build/chia_plot -n 1 -r ${{Threads}} -u ${{Buckets}} -t ${{$Cache_1}}/${{DriveAlias}}/${{PlotterAlias}}/${{PlotterInstance}} -2 ${{$Cache_2}}/${{DriveAlias}}/${{PlotterAlias}}/${{PlotterInstance}} -d ${{$Destination}} -f ${{FarmerKey}} -c ${{PoolContractAddress}} -w",
    "PlotSizes": [
      {
        "Name": "K32",
        "CreationSize": 220.0,
        "FinalSize": 10100.4
      }
    ],
    "DriveClearers": [
      {
        "Name": "OgDate",
        "Arguments": {
          "ThresHold": "07/07/2021",
          "AdditionalDrives": "/mnt/c/; /mnt/g/"
        }
      }
    ]
  },
  "Plotters": [
    {
      "Enabled": true,
      "Alias": "MainPlotter",
      "MaxInstances": 3,
      "Command": {
        "PlotSize": "K32",
        "TotalThreads": 4,
        "TotalRam": 4000,
        "Buckets": 128,
        "PlotCommand": null
      },
      "Work": {
        "Caches": [
          {
            "Directory": "/mnt/g/Cache/Temp",
            "Distribution": 1
          },
          {
            "Directory": "/mnt/g/Cache",
            "Distribution": 0.5
          }
        ],
        "WorkingDirectory": "/mnt/g/App",
        "ArchiveProgressFiles": true,
        "ThrowOnMissingCacheSpace":  true
      },
      "Progress": {
        "Name": "String",
        "Arguments": {
          "TransferExtension": ".plot.tmp",
          "Filter": "plot-"
        }
      },
      "Delay": [
        {
          "Name": "ProgressFile",
          "Arguments": {
            "Filter": "Phase 1 took"
          }
        }
      ]
    }
  ],
  "Drives": [
    {
      "Alias": "ChiaDriveOne",
      "Directory": "/mnt/g/App/Plots",
      "Priority": 1,
      "MaxInstances": 1
    }
  ]
}